{
  "name": "tatler-client",
  "version": "1.0.0",
  "description": "Client for tatler.ekruglov.com notification service",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "lint": "semistandard",
    "releaseit": "release-it --ci",
    "pre-commit": "npm test && npm run lint && npm audit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "npm run pre-commit"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nordluf/tatler-client.git"
  },
  "keywords": [
    "notifications",
    "telegram"
  ],
  "author": "Evgeny Kruglov",
  "license": "JSON",
  "bugs": {
    "url": "https://github.com/nordluf/tatler-client/issues"
  },
  "homepage": "https://github.com/nordluf/tatler-client#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@release-it/bumper": "^1.0.5",
    "@release-it/conventional-changelog": "^1.1.0",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "mocha": "^6.2.0",
    "release-it": "^12.4.3",
    "semistandard": "^14.2.0"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
